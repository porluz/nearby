{
  "name": "react-typescript",
  "version": "1.0.0",
  "description": "React and TypeScript example starter project",
  "keywords": [
    "typescript",
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mdx-js/react": "^2.2.1",
    "@reach/combobox": "0.18.0",
    "@reach/visually-hidden": "0.18.0",
    "@types/google-maps": "3.2.3",
    "@types/node": "^20.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "match-sorter": "6.0.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "^5.0.1",
    "theme-ui": "^0.14.5",
    "use-throttle": "0.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.3",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^2.8.3",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "start": "npm run lint && react-scripts start",
    "lint": "eslint src",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom  --collectCoverage=true --changedSince=main --watchAll=false --passWithNoTests=false --config=jest.config.js",
    "test:coverage-badges": "jest-coverage-badges",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  }
}
